            "type": "docker",
            "settings": {
              "image": "$AGENT",
              "createOptions": {
                "HostConfig":{
                  "PortBindings":{
                    "5671/tcp":[{"HostPort":"5671"}],
                    "8883/tcp":[{"HostPort":"8883"}],
                    "443/tcp":[{"HostPort":"443"}]
                  }
                }
              }
            }
          },
