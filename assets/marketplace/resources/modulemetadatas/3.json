{
    "containerUri": "microsoft/iot-edge-opc-publisher",
    "tagsOrDigests": [
        "latest"
    ],
    "routes": [],
    "twins": [],
    "createOptions": {
        "Hostname": "pub-test",
        "Cmd": [
            "publisher",
            "--pf=./pn.json",
            "--di=60",
            "--to",
            "--aa",
            "--si=0",
            "--ms=0"
        ],
        "HostConfig": {
            "PortBindings": {
                "62222/tcp": [{
                    "HostPort": "62222"
                }]
            },
            "Binds": [
                "x509certstores:/root/.dotnet/corefx/cryptography/x509stores",
                "d:/iiotedge:/appdata"
            ],
            "ExtraHosts": [
                "localhost:127.0.0.1",
                "opctestsvr:192.168.178.26"
            ]
        }
    }
}
